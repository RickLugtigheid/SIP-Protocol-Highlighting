{
  "name": "sip-protocol-highlight",
  "displayName": "SIP hightlight",
  "description": "An extensions for SIP Protocol highlighting",
  "icon": "assets/images/icon.png",
  "version": "0.1.0",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "sip",
      "aliases": ["SIP protocol", "sip"],
      "extensions": [".sip"],
      "configuration": "./language-configuration.json",
      "icon": {
        "dark": "./assets/images/icon.ico",
        "light": "./assets/images/icon.ico"
      }
    }],
    "grammars": [{
      "language": "sip",
      "scopeName": "source.sip",
      "path": "./syntaxes/sip.tmLanguage.json"
    }]
  }
}
